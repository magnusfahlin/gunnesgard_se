    "image-webpack-loader": "^3.4.2",
    "jest": "^22.0.4",
    "jest-cli": "^22.0.4",
    "jest-css-modules": "^1.1.0",
    "mocha": "^4.1.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.3",
    "parallelshell": "^3.0.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.3",
    "style-loader": "^0.19.1",
    "supertest": "^3.0.0",
    "webpack": "^3.10.0",
