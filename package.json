{
  "name": "gunnesgard_se_v2",
  "version": "1.0.0",
  "description": "<h1>Gunnesgard Version 2</h1>",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config ./webpack.config.js/",
    "build:watch:client": "webpack --config ./webpack.config.js/ --watch",
    "build:prod": "node node_modules/webpack/bin/webpack.js --config ./webpack.prod.config.js/",
    "start:dev:client": "webpack-dev-server --env development --open",
    "debug server": "node --nolazy --inspect-brk=9229 server.js",
    "test": "node node_modules/jest-cli/bin/jest.js client/ --clearCache",
    "test:integration": "better-npm-run test:integration",
    "heroku-prebuild": "npm run install -dev",
    "postinstall": "npm run build:prod"
  },
  "betterScripts": {
    "test:integration": {
      "command": "node node_modules/mocha/bin/mocha ",
      "env": {
        "NODE_ENV": "test",
        "TEST_TOKEN": "test",
        "POSTS_SHOWN_NOT_LOGGED_IN": "1"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magnusfahlin/gunnesgard_se_v2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magnusfahlin/gunnesgard_se_v2/issues"
  },
  "homepage": "https://github.com/magnusfahlin/gunnesgard_se_v2#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "better-npm-run": "^0.1.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-mongo-sanitize": "^1.3.2",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "mongodb": "^3.0.0-rc0",
    "mongoose": "^4.13.7",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-multi": "^0.1.12",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-rename-import": "^2.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "expect": "^22.0.3",
    "extract-text-webpack-plugin": "^3.0.2",
